<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../../../stylesheets/page.xsl"?>
<element extends="markup:xul:XULElement" name="page">
	<abstract>
		<locale>
			Similar to a <link type="element" href="reference://markup/xul/window">window</link>, except it should be used for XUL files that are to be loaded into an <link type="element" href="reference://markup/xul/iframe">iframe</link>.
		</locale>
		<locale xml:lang="ru">
			Аналогичен элементу <link type="element" href="reference://markup/xul/window">window</link> за исключением того,
			что он используется для файлов, которые загружаются в <link type="element" href="reference://markup/xul/iframe">iframe</link>.
		</locale>
	</abstract>
	<remarks />
	<examples />
	<members>
		<attributes />
		<methods />
		<properties />
		<events>
			<event name="load" type="dom:events:Event" cancelable="false" bubbles="false">
				<description>
					<locale>
						Fires after element is loaded.
					</locale>
					<locale xml:lang="ru">
						Загорается сразу после того, как ядро загрузит объект.
					</locale>
				</description>
			</event>
			<event name="unload" type="dom:events:Event" cancelable="false" bubbles="false">
				<description>
					<locale>
						Fires before element is unloaded.
					</locale>
					<locale xml:lang="ru">
						Загорается непосредственно перед тем, как объект будет выгружен.
					</locale>
				</description>
			</event>
		</events>
	</members>
</element>